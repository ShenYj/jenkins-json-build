  "编译构建": {
    "执行RN-iOS构建": {
      "Type": "COMMAND_STATUS",
      "Variable": {
        "ProjectName": "Test-RN-iOS-Build",
        "PlistName": "test-rn-iOS-build.plist"
      },
      "Script": {
        "打包": "export LC_ALL=en_US.UTF-8;cd ${PROJECT_PATH}/ios/;xcodebuild -scheme ${ProjectName}  -configuration Release -archivePath build/${ProjectName}.xcarchive archive;xcodebuild -exportArchive -archivePath build/${ProjectName}.xcarchive -exportPath build -exportOptionsPlist ${PlistName}"
      }
    }
  },
